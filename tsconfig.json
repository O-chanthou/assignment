{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": ["@intlify/vite-plugin-vue-i18n/vite"],
    "allowJs": true
  },
  "references": [
    {
      "path": "./tsconfig.config.json",
    }
  ]
}